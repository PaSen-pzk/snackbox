<!--miniprogram/pages/profile/editProfile/editProfile.wxml-->
<view class="editProfile">
  <navigator url="./head/head" hover-class="navigator-hover" open-type="navigate">
    <view class="editProfile-list-item">
      <text>修改头像</text>
      <text class="iconfont iconxiangyoujiantou"></text>
    </view>
  </navigator>
  <navigator url="./name/name" hover-class="navigator-hover" open-type="navigate">
    <view class="editProfile-list-item">
      <text>修改昵称</text>
      <text class="iconfont iconxiangyoujiantou"></text>
    </view>
  </navigator>
  <navigator url="./signature/signature" hover-class="navigator-hover" open-type="navigate">
    <view class="editProfile-list-item">
      <text>修改个性签名</text>
      <text class="iconfont iconxiangyoujiantou"></text>
    </view>
  </navigator>
  <navigator url="./addr/addr" hover-class="navigator-hover" open-type="navigate">
    <view class="editProfile-list-item">
      <text>共享位置</text>
      <text class="iconfont iconxiangyoujiantou"></text>
    </view>
  </navigator>
  <navigator url="./phone/phone" hover-class="navigator-hover" open-type="navigate">
    <view class="editProfile-list-item">
      <text>设置手机号 <text class="editProfile-list-tips">(仅好友可见)</text></text>
      <text class="iconfont iconxiangyoujiantou"></text>
    </view>
  </navigator>
  <navigator url="./weixin/weixin" hover-class="navigator-hover" open-type="navigate">
    <view class="editProfile-list-item">
      <text>设置微信号 <text class="editProfile-list-tips">(仅好友可见)</text></text>
      <text class="iconfont iconxiangyoujiantou"></text>
    </view>
  </navigator>



</view>
